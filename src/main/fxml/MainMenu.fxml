<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenuController">
    <children>
        <VBox prefHeight="450.0" prefWidth="149.0">
            <children>
                <ButtonBar prefHeight="90.0" prefWidth="149.0">
                    <buttons>
                        <Button fx:id="websiteHistory" mnemonicParsing="false" onAction="#onActionWebsiteHistory" prefHeight="90.0" prefWidth="138.0" text="Web history&#10;">
                            <font>
                                <Font name="Nirmala UI" size="18.0" />
                            </font>
                        </Button>
                    </buttons>
                </ButtonBar>
                <ButtonBar prefHeight="90.0" prefWidth="149.0">
                    <buttons>
                        <Button fx:id="blockWebsite" minWidth="0.0" mnemonicParsing="false" onAction="#onActionBlockWebsite" prefHeight="90.0" prefWidth="138.0" text="Block &#10;website">
                            <font>
                                <Font name="Nirmala UI" size="18.0" />
                            </font>
                        </Button>
                    </buttons>
                </ButtonBar>
                <ButtonBar prefHeight="90.0" prefWidth="149.0">
                    <buttons>
                        <Button fx:id="blockedAndUnblock" mnemonicParsing="false" onAction="#onActionBlockedAndUnblock" prefHeight="90.0" prefWidth="138.0" text="Blocked&#10;&amp; unblock">
                            <font>
                                <Font name="Nirmala UI" size="18.0" />
                            </font>
                        </Button>
                    </buttons>
                </ButtonBar>
                <ButtonBar prefHeight="90.0" prefWidth="136.0">
                    <buttons>
                        <Button fx:id="cookies" mnemonicParsing="false" onAction="#onActionCookies" prefHeight="90.0" prefWidth="138.0" text="Cookies">
                            <font>
                                <Font name="Nirmala UI" size="18.0" />
                            </font>
                        </Button>
                    </buttons>
                </ButtonBar>
            <ButtonBar layoutX="10.0" layoutY="280.0" prefHeight="90.0" prefWidth="136.0">
               <buttons>
                  <Button fx:id="contact1" mnemonicParsing="false" onAction="#onActionContact" prefHeight="90.0" prefWidth="138.0" text="Contact">
                     <font>
                        <Font name="Nirmala UI" size="18.0" />
                     </font>
                  </Button>
               </buttons>
            </ButtonBar>
            </children>
        </VBox>
        <Pane layoutX="149.0" prefHeight="450.0" prefWidth="650.0">
            <children>
                <PieChart layoutY="58.0" prefHeight="391.0" prefWidth="650.0" />
                <Label layoutX="14.0" layoutY="6.0" prefHeight="44.0" prefWidth="284.0" text="Top used websites:">
                    <font>
                        <Font name="Nirmala UI" size="18.0" />
                    </font>
                </Label>
            </children>
        </Pane>
    </children>
</AnchorPane>
